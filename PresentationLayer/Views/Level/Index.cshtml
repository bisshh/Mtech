@model List<CoreLayer.Level>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table class="table">
        <thead>
            <tr>
                <td>Year</td>
                <td>Semester</td>
                <td>Description</td>
                <td>Inserted Date</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Year</td>
                    <td>@item.Semester</td>
                    <td>@item.Description</td>
                    <td>@item.InsertedDate</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
