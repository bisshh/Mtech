@model PresentationLayer.Models.UserViewModel
@{
    ViewBag.Title = "SaveUser";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Save User</title>
</head>
<body>
    <h1>Following user data has been saved</h1>
    <div>
        <p>Full Name: @Model.FirstName @Model.MiddleName @Model.LastName</p>
        <p>Username: @Model.Username</p>
        <p>Email: @Model.Email</p>
    </div>
</body>
</html>
