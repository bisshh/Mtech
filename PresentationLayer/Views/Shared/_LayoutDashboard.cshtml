<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="MTech College CMS" />
    <meta name="keywords" content="MTech College CMS" />
    <link href="~/Content/themes/base/images/favicon.png" rel="icon">
    <title>@ViewBag.Title</title>
    <link href="~/Content/themes/base/css/style.min.css" rel="stylesheet">
    <!-- jQuery library -->
    <script src="~/Content/themes/base/js/modernizr.js"></script>
    <script src="~/Content/themes/base/js/jquery.min.js"></script>
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="~/Content/themes/base/js/html5shiv.js"></script>
      <script src="~/Content/themes/base/js/respond.min.js"></script>
    <![endif]-->
    @Scripts.Render("~/bundles/modernizr")
</head>

<body>

        @Html.Partial("~/Views/PartialViews/Navigation.cshtml")

        <div class="col-md-9 main-content">
            <div class="top">
                <div class="row">
                    <div class="col-md-12">
                        <nav class="navbar navbar-default">
                            <div class="navbar-header">
                                <h2 class="navbar-brand" href="#">Welcome Resham to Mtech Dashboard!</h2>
                            </div>
                            <ul class="nav navbar-nav navbar-right">
                                <li class="dropdown">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><i class="fa fa-bell"></i></a>
                                    <ul class="dropdown-menu">
                                        <li><a href="#">Action</a></li>
                                        <li><a href="#">Another action</a></li>
                                        <li><a href="#">Something else here</a></li>
                                        <li role="separator" class="divider"></li>
                                        <li><a href="#">Separated link</a></li>
                                    </ul>
                                </li>
                                <li class="dropdown">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><i class="fa fa-user"></i></a>
                                    <ul class="dropdown-menu">
                                        <li><a href="#">Action</a></li>
                                        <li><a href="#">Another action</a></li>
                                        <li><a href="#">Something else here</a></li>
                                        <li role="separator" class="divider"></li>
                                        <li><a href="#">Separated link</a></li>
                                    </ul>
                                </li>
                            </ul>
                            <form class="navbar-form navbar-right">
                                <div class="form-group">
                                    <input type="text" class="form-control" placeholder="Search">
                                </div>
                                <button type="submit" class="btn btn-default"><i class="fa fa-search"></i></button>
                            </form>
                        </nav>
                    </div>
                </div>
            </div> <!-- /top -->

            @RenderBody()

        </div> <!-- /main content -->

        @Scripts.Render("~/bundles/jquery")
        @RenderSection("scripts", required: false)


    <div class="clearfix"></div>

    <!-- bootstrap js library -->
    <script src="/Content/themes/base/js/bootstrap.min.js"></script>
</body>

</html>
